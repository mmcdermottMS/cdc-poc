{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensions": {
    "eventHubs": {
      "maxEventBatchSize": 1,
      "trackLastEnqueuedEventProperties": true,
      "batchCheckpointFrequency": 1
    }
  }
}